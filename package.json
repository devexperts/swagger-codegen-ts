{
  "name": "@devexperts/swagger-codegen-ts",
  "version": "0.5.0",
  "description": "TS generator for swagger spec",
  "main": "dist/index.js",
  "scripts": {
    "ts-node": "ts-node",
    "tslint": "tslint -c tslint.json --project tsconfig.json './src/**/*.ts' './test/**/*.ts'",
    "test": "yarn tslint && yarn prettier && ts-node ./test/index.ts",
    "prettier": "prettier --list-different \"./src/**/*.ts\" \"./test/**/*.ts\"",
    "prettier:fix": "prettier --write \"./src/**/*.ts\" \"./test/**/*.ts\"",
    "prepublishOnly": "yarn test && yarn build",
    "start": "nodemon",
    "build": "tsc -p ./src",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "version": "yarn changelog && git add CHANGELOG.md"
  },
  "author": "devexperts",
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devex-web-frontend/swagger-codegen-ts.git"
  },
  "bugs": {
    "url": "git+https://github.com/devex-web-frontend/swagger-codegen-ts/issues"
  },
  "homepage": "https://github.com/devex-web-frontend/swagger-codegen-ts#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@devexperts/utils": "^0.28.3",
    "@types/del": "^4.0.0",
    "@types/fs-extra": "^5.0.5",
    "@types/js-yaml": "^3.12.1",
    "@types/prettier": "^1.16.3",
    "del": "^4.1.0",
    "fs-extra": "^7.0.1",
    "js-yaml": "^3.13.1",
    "prettier": "^1.17.0"
  },
  "devDependencies": {
    "fp-ts": "^1.17.1",
    "io-ts": "^1.8.5",
    "io-ts-types": "^0.4.7",
    "typescript": "^3.4.5",
    "@devexperts/lint": "^0.28.3",
    "@devexperts/remote-data-ts": "^0.5.0",
    "@devexperts/rx-utils": "^0.28.3",
    "@types/node": "^11.13.7",
    "conventional-changelog-cli": "^2.0.17",
    "nodemon": "^1.18.11",
    "rxjs": "^6.5.1",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1"
  },
  "peerDependencies": {
    "fp-ts": "^1.17.1",
    "io-ts": "^1.8.5",
    "io-ts-types": "^0.4.7",
    "@devexperts/remote-data-ts": "^0.5.0"
  }
}
