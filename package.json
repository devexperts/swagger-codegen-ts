{
  "name": "@devexperts/swagger-codegen-ts",
  "version": "2.0.0-alpha.0",
  "description": "TS generator for swagger spec",
  "main": "dist/index.js",
  "scripts": {
    "ts-node": "ts-node",
    "test:lint": "eslint ./src/**/*.ts ./test/**/*.ts --fix",
    "test:build": "ts-node ./test/index.ts && tsc -p tsconfig.test.json",
    "test:unit": "jest",
    "test": "yarn test:lint && yarn prettier && yarn test:unit && yarn test:build",
    "prettier": "prettier --list-different \"./src/**/*.ts\" \"./test/**/*.ts\"",
    "prettier:fix": "prettier --write \"./src/**/*.ts\" \"./test/**/*.ts\"",
    "prepublishOnly": "yarn test && yarn build",
    "start": "nodemon --exec \"ts-node ./test/index.ts\"",
    "start:3": "nodemon --exec \"ts-node ./test/3.0.ts\"",
    "build": "tsc -p ./src",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "version": "yarn changelog && git add CHANGELOG.md"
  },
  "author": "devexperts",
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devexperts/swagger-codegen-ts.git"
  },
  "bugs": {
    "url": "git+https://github.com/devexperts/swagger-codegen-ts/issues"
  },
  "homepage": "https://github.com/devexperts/swagger-codegen-ts#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@devexperts/utils": "^1.0.0-alpha.2",
    "@types/del": "^3.0.1",
    "@types/fs-extra": "^8.0.0",
    "@types/js-yaml": "^3.11.2",
    "@types/prettier": "^1.13.2",
    "del": "^3.0.0",
    "fs-extra": "^8.1.0",
    "js-yaml": "^3.13.1",
    "json-schema-ref-parser": "^7.1.1",
    "openapi-types": "^1.3.5",
    "prettier": "^1.15.3",
    "swagger-parser": "^8.0.1"
  },
  "devDependencies": {
    "@devexperts/lint": "^1.0.0-alpha.2",
    "@devexperts/remote-data-ts": "^2.0.0",
    "@devexperts/rx-utils": "^1.0.0-alpha.2",
    "@types/jest": "^24.0.18",
    "conventional-changelog-cli": "^2.0.21",
    "eslint": "^6.5.1",
    "fp-ts": "^2.1.0",
    "io-ts": "^2.0.1",
    "io-ts-types": "^0.5.1",
    "jest": "^24.9.0",
    "nodemon": "^1.18.3",
    "rxjs": "^6.3.3",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  },
  "peerDependencies": {
    "@devexperts/remote-data-ts": "^2.0.0",
    "fp-ts": "^2.1.0",
    "io-ts": "^2.0.1",
    "io-ts-types": "^0.5.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "timers": "fake"
  }
}
